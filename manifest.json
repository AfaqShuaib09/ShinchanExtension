{
    "name": "Shinchan -- Extension",
    "author": "Muhammad Afaq Shuaib",
    "description": "Replace All images",
    "version": "1.0",
    "manifest_version": 2,
    "icons": { "16": "./images/shinchan.jpg",
        "48": "./images/shinchan.jpg",
       "128": "./images/shinchan.jpg" },
  
    "browser_action": {
      "default_title":"Load Images -- Shinchan"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
      
    ]
  }